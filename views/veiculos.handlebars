<body>
   <nav class="navbar navbar-expand-lg bg-light fixed-top">
        <div class="container-fluid">
            <a class="navbar-brand text-success navabar-fixed" href="">Localiza</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-link" href="/">Cadastro de Cliente </a>
                    <a class="nav-link" href="/">Cadastro de Veiculos</a>
                    <a class="nav-link" href="/">Cadastro de Reservas</a>
                    <a class="nav-link" href="/clientes">Clientes cadastrados</a>
                    <a class="nav-link" href="/veiculos">Veiculos cadastrados</a>
                    <a class="nav-link" href="/reservas">Reservas cadastradas</a>
                    <a class="nav-link text-danger" href="/edit">Editar</a>
                    <a class="nav-link text-warning" href="/delete">Delete</a>
                </div>
            </div>
        </div>
    </nav>
     <h1>{{ title }}</h1>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Marca</th>
          <th>Modelo</th>
          <th>Ano</th>
          <th>Placa</th>
          <th>Cor</th>
          <th>Disponibilidade</th>
        </tr>
      </thead>
      <tbody>
        {{#each clientes}}
        <tr>
          <td>{{ id }}</td>
          <td>{{ marca }}</td>
          <td>{{ modelo }}</td>
          <td>{{ ano }}</td>
          <td>{{ placa }}</td>
          <td>{{ cor }}</td>
          <td>{{ disponibilidade }}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
</body>